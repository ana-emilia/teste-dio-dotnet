using System;

namespace DigitalInnovationOne
{
    class Program
    {
        static void Main(string[] args)
        {
            int numerodevezes = 10;

            for (int i = 0; i < numerodevezes; i++)
            {
                Console.WriteLine($"Bem-vindo ao curso de .NET  {i}");
            }
        }
    }
}
